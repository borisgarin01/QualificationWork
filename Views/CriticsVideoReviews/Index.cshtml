@model IEnumerable<CriticVideoReview>
<!DOCTYPE html>
<html>
<body>
    <form class="align-self-baseline" asp-controller="CriticsVideoReviews" asp-action="Index" method="get">
        <a asp-action="Create">Add new critic</a>
        @foreach (CriticVideoReview criticVideoReview in Model)
        {
            <div class="align-self-baseline">
                <h4 class="align-self-baseline">@criticVideoReview.Url</h4><br />
                <h4 class="align-self-baseline">@criticVideoReview.GameId</h4><br />
                <a class="align-self-baseline" asp-action="Delete" asp-route-id="@criticVideoReview.Id">Delete</a>
                <a class="align-self-baseline" asp-action="Get" asp-route-id="@criticVideoReview.Id">Get</a>
                <a class="align-self-baseline" asp-action="Update" asp-route-id="@criticVideoReview.Id">Update</a>
            </div>
        }
    </form>
</body>
</html>