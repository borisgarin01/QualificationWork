@model CriticTextReview
<!DOCTYPE html>
<html>
<body>
    <form class="align-self-baseline" asp-controller="CriticsTextReviews" asp-action="Delete" method="post">
        <h4 class="align-self-baseline">@Model.Text</h4><br />
        <h4 class="align-self-baseline">@Model.GameId</h4><br />
        <h4 class="align-self-baseline">@Model.CriticId</h4><br />
        <input class="align-self-baseline" type="submit" value="Delete" />
    </form>
</body>
</html>